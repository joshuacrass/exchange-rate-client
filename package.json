{
  "name": "react-starter-test-2",
  "version": "1.0.0",
  "main": "index.jsx",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean; webpack --config config/webpack/webpack-prod-config.js",
    "check-staged": "lint-staged",
    "clean": "rm -rf build/*",
    "lint": "eslint src -c .eslintrc --ext js,jsx",
    "sass-lint": "sass-lint -vq -c .sass-lint.yml 'app/**/*.scss'",
    "start:dev": "node config/webpack/webpack-dev-server.js",
    "start:prod": "yarn build; serve build",
    "test:watch": "jest --watch",
    "test": "jest --runInBand"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "path": "^0.12.7",
    "prettier": "^1.14.2",
    "react-test-renderer": "^16.5.0",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^7.1.0",
    "serve": "^10.0.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.2",
    "webpack-command": "^0.4.1",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash.isequal": "^4.5.0",
    "query-string": "^6.1.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-hot-loader": "^4.3.6",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "trend": "^0.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint -c .eslintrc"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --list-different"
    ],
    "app/**/*.scss": [
      "sass-lint -vq"
    ]
  }
}
